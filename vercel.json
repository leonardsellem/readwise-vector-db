{
  "version": 2,
  "buildCommand": "./vercel_build.sh",
  "installCommand": "echo 'Skipping default install, using custom build script'",
  "outputDirectory": ".",
  "framework": null,
  "builds": [
    {
      "src": "api/handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/handler.py"
    }
  ],
  "env": {
    "DEPLOY_TARGET": "vercel",
    "DB_BACKEND": "supabase"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "regions": ["iad1"],
  "trailingSlash": false
}
