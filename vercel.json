{
  "buildCommand": "./vercel_build.sh",
  "installCommand": "echo 'Skipping default install, using custom build script'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "DEPLOY_TARGET": "vercel",
    "DB_BACKEND": "supabase"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "trailingSlash": false
}
